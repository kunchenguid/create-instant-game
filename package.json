{
  "name": "create-instant-game",
  "author": "Kun Chen",
  "version": "0.1.0",
  "license": "MIT",
  "bin": {
    "create-instant-game": "./bin.js"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "tabWidth": 2,
    "importOrder": [
      "^\\."
    ],
    "importOrderSeparation": true
  },
  "module": "dist/create-instant-game.esm.js",
  "size-limit": [
    {
      "path": "dist/create-instant-game.cjs.production.min.js",
      "limit": "512 KB"
    },
    {
      "path": "dist/create-instant-game.esm.js",
      "limit": "512 KB"
    }
  ],
  "peerDependencies": {},
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@types/fs-extra": "^9.0.11",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.2.0",
    "fs-extra": "^9.1.0",
    "moment": "^2.29.1"
  }
}
